{
  "name": "wedding_invitation",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "node scripts/update-version.js",
    "build": "next build",
    "build:fresh": "npm run prebuild && npm run build",
    "start": "next start",
    "lint": "next lint",
    "export": "next build && next export",
    "deploy": "npm run build && touch out/.nojekyll && git add out/ && git commit -m \"Deploy to GitHub Pages\" && git subtree push --prefix out origin gh-pages",
    "optimize-quality": "node scripts/optimize-images-quality-only.js",
    "generate-thumbnails": "node scripts/generate-thumbnails.js"
  },
  "dependencies": {
    "framer-motion": "^12.12.1",
    "next": "15.3.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.18"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/kakao-js-sdk": "^1.39.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/styled-components": "^5.1.34",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
